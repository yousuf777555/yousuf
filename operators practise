2)8(4  /  %
  8
------
  0
-----

  5)15(3
    15
--------
   0
--------

#include <stdio.h>

int main() {
    int a=15,b=5,sum,sub,mul,div,mod;
    sum=a+b;
    sub=a-b;
    mul=a*b;
    div=a/b;
    mod=a%b;
    printf("addition is %d\n",sum);
    printf("substraction is %d\n",sub);
    printf("multiplication is %d\n",mul);
    printf("division is %d\n",div);
    printf("modular division is %d\n",mod);

    return 0;
}


#include<stdio.h>
int main()
{
    int a=6,b=9;
    printf("%d\n",a>b);
    printf("%d\n",a>=b);
    printf("%d\n",a<b);
    printf("%d\n",a<=b);
    printf("%d\n",a==b);
    printf("%d\n",a!=b);
}



#include<stdio.h>
int main()
{
    int a=8,b=9;
    printf("%d\n",(a>b)&&(a!=b));
    printf("%d\n",(a>b)||(a!=b));
    printf("%d\n",!(a>b)
}


#include<stdio.h>
int main()
{
    int a=8,b=9;
   
    printf("%d\n",a+=b);      //a=a+b;   //a+=8// a=5   b=6  a+=b// a=5+6
    printf("%d\n",a*=b);       //a=a*b;
}




#include <stdio.h>

int main() {
    int a=4,b;
    b=++a;
    printf("%d",a);
    
    return 0;
}



#include <stdio.h>

int main() {
    int a=4;
    a = a++ + ++a + a--;
    printf("%d",a);
    
    return 0;
}



#include <stdio.h>

int main() {
  int a=2,b=5,s;
  s=a>b?a:b;
  printf("%d",s);
}



  comma operator 

#include <stdio.h>

int main()
{
	int a,b;
	a = 10,20,30;
	b = (10,20,30);
	//printing the values
	printf("a= %d, b= %d\n",a,b);
	return 0;
}


#include<stdio.h>
int main() {
    int intType;
    float floatType;
    double doubleType;
    char charType;

    // sizeof evaluates the size of a variable
    printf("Size of int: %zu bytes\n", sizeof(intType));
    printf("Size of float: %zu bytes\n", sizeof(floatType));
    printf("Size of double: %zu bytes\n", sizeof(doubleType));
    printf("Size of char: %zu byte\n", sizeof(charType));
    
    return 0;
}



